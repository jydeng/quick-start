{
  "name": "quick-start",
  "version": "0.0.1",
  "description": "quick start develop a webApp!",
  "main": "main.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server --open --config ./config/webpack.config.dev.js",
    "serve:https": "cross-env NODE_ENV=development webpack-dev-server --open --config ./config/webpack.config.dev.https.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.prod.js",
    "analyze:size": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.analyze.size.js",
    "analyze:speed": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.analyze.speed.js",
    "commit": "git-cz"
  },
  "keywords": [
    "webpack",
    "quick-start"
  ],
  "author": "jydeng",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@babel/runtime-corejs3": "^7.11.2",
    "@intervolga/optimize-cssnano-plugin": "^1.0.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.5.0",
    "img-loader": "^3.0.2",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^1.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.4",
    "postcss-url": "^8.0.0",
    "precss": "^4.0.0",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^5.0.0",
    "tailwindcss": "^1.9.4",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "dependencies": {
    "vue": "^2.6.12",
    "vuex": "^3.5.1",
    "vue-router": "^3.4.6"
  },
